            <span class="k">auto</span> <span class="n">itr</span> <span class="o">=</span> <span class="n">ump</span><span class="p">.</span><span class="n">find</span><span class="p">(</span><span class="n">another</span><span class="p">);</span>
            <span class="k">if</span> <span class="p">(</span><span class="n">end</span><span class="p">(</span><span class="n">ump</span><span class="p">)</span> <span class="o">!=</span> <span class="n">itr</span><span class="p">)</span> <span class="p">{</span>
